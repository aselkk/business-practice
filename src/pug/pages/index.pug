extends ../layout.pug
include ../mixins/problems
block content

    section.hero
        .container.container__hero.d-md-flex.align-items-center
            .row.hero-row.d-flex.justify-content-space-between.col-md-12
                .col-md-6.col-sm-6.hero__text
                    .heading
                        img.bg-polygon(src="assets/img/bg-polygons.png", alt="cubez")
                        h2.hero__title Создаем решения
                        h3.hero__title.hero__title__bottom Для роста бизнеса
                    p.hero__subtitle На основе анализа данных с алгоритмами
                        br
                        | реализации
                    button.open-modal-btn.main__btn.main__btn__hero(data-target="modal-window") обсудить проблему
                .col-md-5.col-sm-6.hero__img
                    img(src="assets/img/main-cubes.png", alt="cubez")
                    

    img.arrows(src="assets/img/arrows.png", alt="arrows")
    section.problems
    .container.container__problems.d-sm-flex.align-items-center

        .col-sm-6.problems
            +problems('Как найти точки роста и определить цели?')
            +problems('Как избежать проблем на старте нового бизнеса?')
        .col-sm-6.problems
            +problems('Как увеличить прибыль, управляя ценами?')
            +problems('Как не попасть в "черную дыру" затрат на продвижение?')
